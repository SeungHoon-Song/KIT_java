package sec04.exam01;

public class UsingThis {
	public int field = 10;
	
	class Inner{
		int field = 20;
		
		void method() {
			MyFunctionalInterface fi = () -> {
				System.out.println("outterField: "+UsingThis.this.field);
				System.out.println("innerField: "+field);
			};
			fi.method();
		}
	}

}
