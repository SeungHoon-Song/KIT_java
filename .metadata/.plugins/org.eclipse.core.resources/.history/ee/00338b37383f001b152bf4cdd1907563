package p2;

public class UsingThis {
	public int outterField = 10;
	
	class Inner{
		int innerField = 20;
		
		void method() {
			MyFunctionInterface fi = () ->{
				System.out.println("outterField : "+outterField);
				System.out.println("outterField : "+UsingThis.this.outterField);
			};	//; 익명 객체
			fi.method1(); //메소드 호출
		}
	}
}
